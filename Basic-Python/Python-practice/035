"""
实例 035：设置输出颜色 题目 文本颜色设置
"""
class bcolors:
    HEADER = '\033[95m'
    OKBLUE = '\033[94m'
    OKGREEN = '\033[92m'
    WARNING = '\033[93m'
    FAIL = '\033[91m'
    ENDC = '\033[0m'
    BOLD = '\033[1m'
    UNDERLINE = '\033[4m'

print(bcolors.BOLD + "警告的字体颜色？" + bcolors.WARNING)
print(bcolors.HEADER + "警告的字体颜色？" + bcolors.UNDERLINE)
